<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class name="net.fenyo.gnetwatch.data.EventList" table="EVENT_LIST">
    <id name="id" column="ID">
      <generator class="increment"/>
    </id>
  </class>

    <!-- we should have a one-to-many association to instances of class EventGeneric,
         but for performance issues, we must use the opposite direction instead -->

</hibernate-mapping>
