<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class name="net.fenyo.gnetwatch.IPQuerier" table="IP_QUERIER">

    <id name="id" column="ID">
      <generator class="increment"/>
    </id>

    <property name="tos" not-null="true"/>
    <property name="address" not-null="true"/>
    <property name="portSrc" not-null="true"/>
    <property name="portDst" not-null="true"/>
    <property name="PDUMaxSize" not-null="true"/>
    <property name="useProxy" not-null="true"/>
    <property name="reconnect" not-null="true"/>
    <property name="proxyHost" not-null="true"/>
    <property name="proxyPort" not-null="true"/>
    <property name="URL" not-null="true"/>
    <property name="NParallel" not-null="true"/>

  </class>

</hibernate-mapping>
